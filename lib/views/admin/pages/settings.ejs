<div id="addSettingsPage" class="container-fluid">

    <div class="row">
        <div class="col-sm-12">
            <div class="page-title-box">
                <h4 class="page-title">
                    <%= title %>
                </h4>


            </div>
        </div>
    </div>
    <!-- end row -->

    <div class="row">
        <div class="col-lg-12">
            <div class="card m-b-20">
                <div class="card-body">
                    <p class="text-muted m-b-30 "></p>

                    <form class="" action="<%= MAIN_URL %>/admin/settings" method="POST" enctype="multipart/form-data">


                        <% settings.forEach(s=>{ %>
                            <%- printSettingHTML(s) %>
                                <% })%>

                                    <div class="form-group mb-0">
                                        <div>
                                            <button type="submit" class="btn btn-primary waves-effect waves-light ml-1">
                                    ثبت
                                </button>
                                            <a href="/admin/settings" class="btn btn-secondary waves-effect">
                                    انصراف
                                </a>
                                        </div>
                                    </div>
                    </form>

                </div>
            </div>
        </div>
        <!-- end col -->

    </div>
    <!-- end row -->

</div>
<!-- container-fluid -->